{
  "name": "react-redux-starter",
  "version": "0.2.2",
  "description": "Starter boilerplate for a React Redux application",
  "main": "dist",
  "repository": {
    "url": "git@github.com:celikmus/react-redux-starter.git",
    "type": "git"
  },
  "author": "Mustafa Celik <celikmus@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.5.2",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^17.0.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "case-sensitive-paths-webpack-plugin": "^1.1.4",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.26.0",
    "dotenv": "^2.0.0",
    "enzyme": "^2.5.1",
    "eslint": "^3.10.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-standard": "^6.2.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-promise": "^3.3.2",
    "eslint-plugin-react": "^6.6.0",
    "eslint-plugin-standard": "^2.0.1",
    "file-loader": "^0.9.0",
    "jest": "^17.0.0",
    "json-server": "^0.9.0",
    "node-sass": "^3.11.2",
    "object-assign": "^4.1.0",
    "open": "^0.0.5",
    "postcss-loader": "^1.0.0",
    "promise": "^7.1.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.1.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "stylelint": "^7.5.0",
    "stylelint-config-standard": "^15.0.0",
    "stylelint-webpack-plugin": "^0.4.0",
    "svgo": "^0.7.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2",
    "yarn": "^0.17.6"
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "webpack-dev-server --config ./webpack.config.dev.js --inline --hot",
    "posttest": "yarn run lint",
    "lint": "eslint src",
    "test": "node scripts/test.js --env=jsdom",
    "build": "webpack --config ./webpack.config.prod.js",
    "api": "json-server --watch ./data/db.json --port 3001"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "testPathIgnorePatterns": [
      "./(build|docs|node_modules)/"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$"
  },
  "dependencies": {
    "normalizr": "^2.2.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-actions": "^1.0.0",
    "redux-xhr-middleware": "0.6.0"
  }
}
